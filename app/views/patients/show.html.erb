<style>
	.wrapper {
	  display: grid;
	  grid-template-columns: 1fr 25vw 25vw 25vw 1fr;
	  grid-template-rows: 100px 40vh 50px ;
	  grid-gap: 10px;
	  background-color: #fff;
	  color: #444;
	}
	.a {
	    
	    font-size: 4vh;
	    color: black;
		grid-column-start: 3;
		grid-column-end: 5;
		grid-row-start: 2;
		grid-row-end: 3;
	}
	.b {
		grid-column-start: 1;
		grid-column-end: 6;
		grid-row-start: 3;
		grid-row-end: 4;
		border-bottom: solid;
	}
	.c {
		text-align: center;
		line-height: 60px;
		font-size: 44px;
		color: black;
		grid-column-start: 3;
		grid-column-end: 4;
		grid-row-start: 1;
		grid-row-end: 2;
	}
	.d {
		color:black;
		text-align:center;
		line-height:40vh;
		grid-column-start: 2;
		grid-column-end: 3;
		grid-row-start: 2;
		grid-row-end: 3;
	}
</style>




	<% if current_user.id == @patient.id %>
		

<div class="wrapper">
   <div class="c">My Appointments</div>
		<% if !Appointment.find_by(patient_id:@patient.id).nil? %>
			<% @appointments = Appointment.find_by(patient_id:@patient.id)%> 
			<% @appointments.each do |x| %>

  				<div class="a">
					<%= x.provider.treatment%>
				    <%= x.date%>
					<%= x.start_time%>
					<%= x.end_time%>
					<%= x.provider.name%>
					<%= x.provider.price%>
				</div>

  				<div class="d">
					<% if x.provider.image != [] or nil%> 
						<img src=<%=x.provider.image.first%>> <br/> 
					<%end%> 
					<% if x.provider.qualification != [] or nil%>
						<% if x.provider.qualification.include?".pdf" %>
							<embed src="https://drive.google.com/viewerng/viewer?embedded=true&url=<%= @provider.qualification.first %>" width="500" height="375">
						<% else %>
							<img src=<%= x.provider.qualification.first %> > <br/>
						<%end%> 
					<%end%> 
					<%= link_to 'Provider profile', provider_path(x.provider) %>
				</div>
  				<div class="b"></div>
			<%end%>
		<%else%>
			<div class="a"><p> You have no appointments</p></div> <div class="d"> <img src="data:image/jpeg;base64,"></div>
		<%end%>
</div>
		<%= link_to 'Edit', edit_patient_path(@patient) %> |
		<%= link_to 'Back', patients_path %>
	<% else %>
		  <% controller.redirect_to welcome_index_path %>
	<% end %>
